{
  "firebase": "todo-react-redux",
  "public": "target",

  "headers": [
    {
      "source": "**/*",
      "headers": [
        {"key": "X-Content-Type-Options", "value": "nosniff"},
        {"key": "X-Frame-Options", "value": "DENY"},
        {"key": "X-UA-Compatible", "value": "ie=edge"},
        {"key": "X-XSS-Protection", "value": "1; mode=block"}
      ]
    }
  ],

  "rewrites": [{
    "source": "**",
    "destination": "/index.html"
  }]
}
