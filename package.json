{
  "name": "todo-react-redux",
  "version": "0.4.0",
  "description": "Todo app with React, Redux, and Firebase",
  "homepage": "https://github.com/r-park/todo-react-redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-park/todo-react-redux.git"
  },
  "author": {
    "name": "Richard Park",
    "email": "objectiv@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=5.2"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks --display-modules --progress",
    "clean": "del-cli ./target",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "lint": "eslint -c .eslintrc.yml *.js ./src ./server",
    "prebuild": "npm-run-all clean lint test",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' ./server/main.js",
    "start": "npm run dev",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start"
  },
  "devDependencies": {
    "autoprefixer": "~6.3.6",
    "babel-core": "~6.8.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "~6.2.4",
    "babel-plugin-react-transform": "~2.0.2",
    "babel-polyfill": "~6.8.0",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-1": "~6.5.0",
    "classnames": "~2.2.5",
    "cross-env": "~1.0.7",
    "css-loader": "~0.23.1",
    "del-cli": "~0.2.0",
    "eslint": "~2.9.0",
    "eslint-plugin-react": "~5.0.1",
    "express": "~4.13.4",
    "extract-text-webpack-plugin": "~1.0.1",
    "firebase": "~2.4.2",
    "history": "~2.1.1",
    "html-webpack-plugin": "~2.16.1",
    "jasmine-core": "~2.4.1",
    "karma": "~0.13.22",
    "karma-chrome-launcher": "~1.0.1",
    "karma-jasmine": "~1.0.2",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.7.0",
    "minx": "~0.4.14",
    "mockfirebase": "~0.12.0",
    "moment": "~2.13.0",
    "morgan": "~1.7.0",
    "node-sass": "~3.7.0",
    "nodemon": "~1.9.2",
    "npm-run-all": "~1.8.0",
    "postcss-loader": "~0.9.1",
    "react": "~15.0.2",
    "react-addons-test-utils": "~15.0.2",
    "react-dom": "~15.0.2",
    "react-redux": "~4.4.5",
    "react-router": "~2.4.0",
    "react-router-redux": "~4.0.4",
    "react-transform-hmr": "~1.0.4",
    "redux": "~3.5.2",
    "redux-thunk": "~2.0.1",
    "sass-loader": "~3.2.0",
    "sinon": "~1.17.4",
